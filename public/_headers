# Built assents that have a hash in the file name
/_astro/*
  Cache-Control: public, max-age=31536000, immutable

# Public assets that will have the same name, but content may change
/favicon.ico
  Cache-Control: public, max-age 86400, stale-while-revalidate 172800, stale-if-error 259200

/favicon.svg
  Cache-Control: public, max-age 86400, stale-while-revalidate 172800, stale-if-error 259200

/manifest.webmanifest
  Cache-Control: public, max-age 86400, stale-while-revalidate 172800, stale-if-error 259200